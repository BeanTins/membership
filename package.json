{
  "name": "membership",
  "version": "0.1.0",
  "bin": {
    "membership": "bin/membership.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "test:component": "jest --projects jest.component.config.ts",
    "test:unit": "jest --projects jest.unit.config.ts",
    "deploy:pipeline": "npx cdk deploy",
    "deploy:dev": "npx cdk deploy MembershipDevStage/* --outputs-file awsDeploy.json --app \"npx ts-node --prefer-ts-exts provisioning/membership-dev.ts\""
  },
  "devDependencies": {
    "@aws-cdk/assertions": "^1.134.0",
    "@aws-cdk/aws-apigateway": "^1.134.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.134.0",
    "@types/jest": "^26.0.24",
    "@types/node": "10.17.27",
    "aws-cdk": "2.0.0",
    "esbuild": "^0.14.1",
    "fs": "^0.0.1-security",
    "got": "^11.8.3",
    "jest": "^26.4.2",
    "jest-cucumber": "^3.0.1",
    "jest-junit": "^13.0.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "winston": "^3.3.3"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda": "^1.134.0",
    "@aws-cdk/pipelines": "^1.134.0",
    "@types/aws-lambda": "^8.10.85",
    "aws-cdk-lib": "2.0.0",
    "constructs": "^10.0.0",
    "jsonpath-plus": "^6.0.1",
    "source-map-support": "^0.5.16",
    "typescript": "^4.5.2"
  }
}
